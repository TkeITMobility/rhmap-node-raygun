{
  "name": "rhmap-raygun-nodejs",
  "version": "0.3.0",
  "description": "a wrapper around raygun for use in rhmap node.js applications",
  "main": "index.js",
  "scripts": {
    "unit": "mocha *.test.js -t 5000",
    "test": "eslint *.js && npm run cover && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "cover": "nyc --reporter=lcov --produce-source-map=true npm run unit"
  },
  "keywords": [
    "rhmap",
    "raygun",
    "nodejs",
    "node.js",
    "node"
  ],
  "author": "Evan Shortiss",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "env-var": "~2.4.2",
    "raygun": "~0.9.1",
    "tke-logger": "tkeitmobility/tke-cloud-logger#v0.5.0"
  },
  "engines": {
    "node": ">=4.4.3"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "chai-truthy": "~1.0.0",
    "eslint": "~3.18.0",
    "expect": "~1.20.2",
    "mocha": "~3.2.0",
    "nyc": "~10.1.2",
    "proxyquire": "~1.7.11",
    "sinon": "~1.17.7",
    "sinon-as-promised": "~4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TkeITMobility/rhmap-node-raygun.git"
  },
  "bugs": {
    "url": "https://github.com/TkeITMobility/rhmap-node-raygun/issues"
  },
  "homepage": "https://github.com/TkeITMobility/rhmap-node-raygun#readme"
}
